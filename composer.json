{
  "name": "rnix/bubble-sorter",
  "type": "library",
  "description": "An over-engineered implementation of bubble sort",
  "keywords": ["bubble", "sort", "solid"],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Nix"
    }
  ],
  "scripts": {
    "phpcs": "phpcs -- --standard=PSR12 src tests"
  },
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "BubbleSorter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BubbleSorter\\Tests\\": "tests/"
    }
  }
}
